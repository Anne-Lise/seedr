<h1> Your New seed</h1>

<%= simple_form_for @seedpack do |f| %>
<%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name,
                required: true,
                autofocus: true %>
    <%= f.input :description,
                required: true %>
    <%= f.input :price,
                required: true %>
    <%= f.input :number,
                required: true %>
     <%= f.input :image,
                required: true %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Add" %>
  </div>
<% end %>


<%= link_to  "Go back to list of seeds", seedpacks_path %>
<%#= render "devise/shared/links" %>

